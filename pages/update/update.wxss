
page {
  background: #EFEFEF;
}
.white{
  background: white;
}
.title {
  font-weight: bold;
}
.info {
  font-size: 28rpx;
  width: 100%;
  margin-top: 10rpx;
}
.input_box {
  border-bottom: 1rpx solid #DFDEDF;
  padding: 0 30rpx;
}
.update_img {
  display: flex;
  padding: 30rpx;
}
.update_img image {
  width: 100rpx;
  height: 100rpx;
  border-radius: 10rpx;
  margin-left: 20rpx;
}
.box-show {
  margin-top: 10rpx;
  width: 100%;
  background: #F0F0F0;
  height: 35rpx;
}
.icon {
  width: 60rpx;
  height: 60rpx;
  border-radius: 60rpx;
  margin-right: 20rpx;
}
.monty_box {
  display: flex;
  align-items: center;
  font-weight: bold;
  padding-bottom: 20rpx;
  border-bottom: 1rpx solid #BDBABA;
}
.chanck {
  padding: 10rpx 20rpx;
}
.num {
  display: flex;
  margin-top: 10rpx;
  align-items: center;
}
.Agreement {
  color: #38ADFF;
  display: flex;
  justify-content: center;
  align-items: center;
  font-size: 28rpx;
  margin-top: 50rpx;
}
.set {
  width: 80%;
  height: 100rpx;
  border-radius: 10rpx;
  background: #B2B2B2;
  color: white;
  margin:60rpx auto;
}
.type {
  font-size: 28rpx;
}